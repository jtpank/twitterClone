<%- include('includes/header') %>

<div class="container py-md-5 container--narrow">
  <%- include('includes/flash')%>
  
    <%- include('includes/profileShared') %>
    <div class="list-group">
     <% posts.forEach(function(post) { %>
      <a href="/post/<%= post._id %>" class="list-group-item list-group-item-action">
        <strong><%= post.title %></strong> <%= post.createdDate.getMonth()+1%>/<%= post.createdDate.getDay()%>/<%= post.createdDate.getFullYear()%>
      </a>
     <% })  %>
    </div>
  </div>

  <%- include('includes/footer') %>